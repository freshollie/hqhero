{
  "name": "hqhero",
  "version": "1.1.0",
  "scripts": {
    "lerna": "lerna",
    "build": "lerna run build",
    "build:server": "lerna exec 'yarn build' --scope=hqhero-server",
    "build:client": "lerna exec 'yarn build' --scope=hqhero-client",
    "ng": "ng",
    "ts-node": "ts-node",
    "start": "ng build && ts-node -P server/tsconfig.json server/server.ts",
    "start-server": "ts-node -P server/tsconfig.json server/server.ts",
    "server": "ts-node -P server/tsconfig.json server/server.ts",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "lerna bootstrap"
  },
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.8",
    "@types/ws": "^6.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lerna": "^3.15.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": ">=2.7.2 <2.10"
  }
}
